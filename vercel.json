{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "COINMARKETCAP_API_KEY": "@coinmarketcap_api_key",
    "ETHERSCAN_API_KEY": "@etherscan_api_key",
    "DISCORD_WEBHOOK_URL": "@discord_webhook_url"
  }
}